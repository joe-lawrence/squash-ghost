{
  "name": "squash-workout-parser",
  "version": "1.1.0",
  "description": "A comprehensive JavaScript library for parsing, validating, and generating squash workout timelines. Designed for integration with the Squash Ghost webapp.",
  "type": "module",
  "main": "squash-workout-parser.js",
  "exports": {
    ".": "./squash-workout-parser.js",
    "./data-structures": "./data-structures.js",
    "./config": "./config.js",
    "./validation": "./validation.js",
    "./timing": "./timing.js",
    "./parser": "./parser.js",
    "./error-handling": "./error-handling.js",
    "./import-export": "./import-export.js",
    "./file-operations": "./file-operations.js"
  },
  "scripts": {
    "test": "node test-runner.js",
    "test:data": "node test-data-structures.js",
    "test:config": "node test-config.js",
    "test:validation": "node test-validation.js",
    "test:timing": "node test-timing.js",
    "test:parser": "node test-parser.js",
    "test:error": "node test-error-handling.js",
    "test:import": "node test-import-export.js",
    "test:file": "node test-file-operations.js",
    "lint": "eslint \"*.js\" --ext .js",
    "lint:fix": "eslint \"*.js\" --ext .js --fix",
    "format": "prettier --write \"*.js\"",
    "format:check": "prettier --check \"*.js\"",
    "check": "npm run lint && npm run format:check && npm test"
  },
  "keywords": [
    "squash",
    "workout",
    "parser",
    "timeline",
    "validation",
    "configuration",
    "javascript",
    "es6",
    "modules"
  ],
  "author": "Joe Lawrence",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/joe-lawrence/squash-ghost-python.git"
  },
  "bugs": {
    "url": "https://github.com/joe-lawrence/squash-ghost-python/issues"
  },
  "homepage": "https://joe-lawrence.github.io/squash-ghost/squash-ghoster/",
  "engines": {
    "node": ">=14.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "eslint": "^9.31.0",
    "globals": "^16.3.0",
    "prettier": "^3.6.2"
  }
}
